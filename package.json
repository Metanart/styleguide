{
  "name": "badoo-styleguide",
  "version": "0.0.51",
  "description": "Badoo styleguide used to develop UI components for the Web and React Native",
  "author": "badoo",
  "license": "MIT",
  "bin": "./webpack-server.js",
  "scripts": {
    "deps": "yarn install --pure-lockfile",
    "build": "yarn start --config=styleguide.config.js --buildDir=./dist/",
    "start": "yarn deps && node ./webpack-server.js",
    "lint": "eslint . --ext .js,.jsx",
    "test": "jest",
    "test-update": "jest -u",
    "prettier": "prettier --write '**/*.{jsx,js,ts,tsx,json}'",
    "styleguide": "yarn start --config=styleguide.config.js"
  },
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.0",
    "@hot-loader/react-dom": "^16.8.4",
    "babel-loader": "8.0.5",
    "cache-loader": "^4.1.0",
    "css-loader": "2.1.0",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "loader-utils": "^1.2.3",
    "metro-react-native-babel-preset": "^0.53.0",
    "react-docgen": "5.3.0",
    "react-docgen-typescript": "^1.16.3",
    "react-hot-loader": "4.8.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "styled-components": "5.0.1",
    "styled-normalize": "8.0.7",
    "ts-loader": "6.2.1",
    "typescript": "3.7.5",
    "typescript-react-function-component-props-handler": "^1.0.1",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.2.1",
    "webpack-merge": "^4.2.1",
    "yargs": "13.2.1"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@types/react": "16.8.4",
    "coveralls": "^3.0.11",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "6.0.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-react": "7.14.2",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "16.8.4",
    "react-test-renderer": "^16.13.1",
    "sinon": "^9.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier"
    }
  },
  "resolutions": {
    "js-yaml": "3.13.1",
    "node-sass/**/tar": "2.2.2"
  }
}
